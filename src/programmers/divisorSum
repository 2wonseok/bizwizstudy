package programmers;

public class stringPY {

    public static void main(String[] args) {
		int sum = solution(5);
        System.out.println(sum);
    }
    public int solution(int n) {
        int answer = 0;
        for(int i=1; i<=n; i++){
            if(n%i==0){
                answer+=i;
            }  
        }
    return answer;
    }

}

